<!--pages/login/login.wxml-->
<view class="container">
  <view class="login-title">登录</view>
  <input class="login-input" type="text" placeholder="手机号" bindinput="onPhoneInput" />
  <input class="login-input" type="password" placeholder="密码" bindinput="onPasswordInput" />
  <button class="login-button" bindtap="onLogin">登录</button>
  <view class="login-register-link" bindtap="onRegister">注册账号</view>
</view>

